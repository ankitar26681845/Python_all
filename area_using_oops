# using oops to find the area
class Area:
    def __init__(self,length,breadth):
        self.length = length
        self.breadth = breadth
    def area_of_rectangle(self):
        area = length*breadth
        print(area)
length  =int(input("enter the length"))
breadth = int(input("enter the breadth"))

a = Area(length,breadth)
a.area_of_rectangle()



